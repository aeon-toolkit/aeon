    "    \" estimators\"\n",
    "   ],\n",
    "   \"metadata\": {\n",
    "    \"collapsed\": false\n",
    "   }\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": 27,\n",
    "   \"outputs\": [\n",
